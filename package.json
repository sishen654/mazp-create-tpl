{
  "name": "@mazp/create-tpl",
  "version": "1.6.1",
  "author": "sishen",
  "license": "MIT",
  "description": "CLI to create project template",
  "keywords": [
    "cli",
    "create",
    "template"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/sishen654/mazp-create-tpl/issues"
  },
  "homepage": "https://github.com/sishen654/mazp-create-tpl",
  "repository": "https://github.com/sishen654/mazp-create-tpl",
  "type": "module",
  "bin": {
    "cra": "index.js"
  },
  "scripts": {
    "watch": "cross-env NODE_ENV=development gulp",
    "w": "pnpm build --watch",
    "build": "rimraf dist & cross-env NODE_ENV=production rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "v": "standard-version -r",
    "t": " git checkout main & git push gitee main --tags & git push github main --tags",
    "p": "git checkout main & git push gitee main & git push github main"
  },
  "files": [
    "index.js",
    "template",
    "dist",
    "lib"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.3",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@types/fs-extra": "9.0.13",
    "@types/inquirer": "9.0.2",
    "@types/node": "18.11.4",
    "cross-env": "7.0.3",
    "execa": "6.1.0",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "gulp-uglify": "3.0.2",
    "happy-dom": "7.6.6",
    "nodemon": "2.0.20",
    "rimraf": "3.0.2",
    "rollup": "3.27.0",
    "rollup-plugin-dts": "5.3.1",
    "rollup-plugin-esbuild": "5.0.0",
    "standard-version": "9.5.0",
    "typescript": "4.8.4",
    "vite": "^5.0.10",
    "vitest": "0.24.3"
  },
  "dependencies": {
    "chalk": "5.1.2",
    "fs-extra": "10.1.0",
    "inquirer": "9.1.4"
  }
}
