    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "gulp-uglify": "3.0.2",
    "happy-dom": "7.6.6",
    "nodemon": "2.0.20",
    "rimraf": "3.0.2",
    "standard-version": "9.5.0",
    "typescript": "4.8.4",
